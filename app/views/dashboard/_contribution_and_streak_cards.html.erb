<div class="row">
 <div class="col s12 m4">
   <div class="card blue-grey darken-1">
     <div class="card-content white-text">
       <div class="col m12">
         <span class="card-title center"></span>
         <div class="contributions center">
           Contributions in the last year
           <div class="contribution-streak-number center">
             <%= @user.contributions_in_last_year %>
           </div>
         </div>
       </div>
     </div>
     <div class="card-action">
     </div>
   </div>
 </div>

 <div class="col s12 m4">
   <div class="card blue-grey darken-1">
     <div class="card-content white-text">
       <div class="col m12">
         <span class="card-title center"></span>
         <div class="longest-streak center">
           Longest Streak
           <div class="contribution-streak-number center">
             <%= @user.longest_streak %>
           </div>
         </div>
       </div>
     </div>
     <div class="card-action">
     </div>
   </div>
 </div>

 <div class="col s12 m4">
   <div class="card blue-grey darken-1">
     <div class="card-content white-text">
       <div class="col m12">
         <span class="card-title center"></span>
         <div class="current-streak center">
           Current Streak
           <div class="contribution-streak-number center">
             <%= @user.current_streak %>
           </div>
         </div>
       </div>
     </div>
     <div class="card-action">
     </div>
   </div>
 </div>
</div>
